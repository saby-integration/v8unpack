	ПараметрыРезультат.Вставить(""Результат"", СбисОтправитьИОбработатьКоманду(Кэш, ""AuthenticateByCertificate"", Новый Структура(""Сертификат"", сбисПараметрыВызова), ДопПараметры, Отказ));
