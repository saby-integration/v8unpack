		certificate.Write(""КодАктивации"", Сертификат.КодАктивации);
		Object = Кэш.docflow.ActivateCertificate(certificate);
		Если Object = 0 Тогда //почему неопределено
			сбисСообщитьОбОшибке();
			Возврат Ложь;
		КонецЕсли;
	КонецЦикла;
	Возврат Истина;
